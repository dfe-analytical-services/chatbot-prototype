{
  "name": "chatbot-ui",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "scripts": {
    "dev": "next dev -p 3002",
    "build": "next build",
    "start": "next start -p 3002",
    "lint": "eslint \"**/*.+(ts|js|tsx)\"",
    "tsc": "tsc --noEmit",
    "test": "tsc --noEmit && eslint \"**/*.+(ts|js|tsx)\" && jest",
    "test-no-check": "jest",
