    "react-markdown": "^9.0.0",
    "sass": "^1.68.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@swc/core": "^1.3.100",
    "@swc/jest": "^0.2.29",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.1",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.7.1",
    "@types/react": "^18.2.23",
    "@types/react-dom": "^18.2.8",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "8.50.0",
    "eslint-config-next": "13.5.3",
    "jest": "^29.7.0",
